{
  "dxt_version": "0.2",
  "name": "@llamaindex/llama-cloud-mcp",
  "version": "1.2.0",
  "description": "The official MCP Server for the Llama Cloud API",
  "author": {
    "name": "Llama Cloud"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/run-llama/llama-cloud-ts.git"
  },
  "homepage": "https://github.com/run-llama/llama-cloud-ts/tree/main/packages/mcp-server#readme",
  "documentation": "https://developers.llamaindex.ai/",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "LLAMA_CLOUD_API_KEY": "${user_config.LLAMA_CLOUD_API_KEY}"
      }
    }
  },
  "user_config": {
    "LLAMA_CLOUD_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
